#include<iostream>
using namespace std;
template <class T>
int swap_numbers(T& X,T& Y)
{
    T temp;
    temp=X;
    X=Y;
    Y=temp;
    return 0;
}
int main()
{
    int a,b;
    a=100,b=200;
    float i=10.1,j=100.1;
    char p='P',r='R';
    cout<<"Before swap- "<<endl;
    cout<<"a="<<a<<","<<"b="<<b<<endl;
    cout<<"i="<<i<<","<<"j="<<j<<endl;
    cout<<"p="<<p<<","<<"r="<<r<<endl;
    swap_numbers(a,b);
    swap_numbers(i,j);
    swap_numbers(p,r);
    cout<<"After swap- "<<endl;
    cout<<"a="<<a<<","<<"b="<<b<<endl;
    cout<<"i="<<i<<","<<"j="<<j<<endl;
    cout<<"p="<<p<<","<<"r="<<r<<endl;
    cout<<"Swapping Completed Successfully";
    return 0;
}
